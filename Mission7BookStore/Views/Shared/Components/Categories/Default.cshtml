
@{

}
@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-bookCategory="" asp-route-pageNum="1"class="@(ViewBag.SelectedType == null ? "btn btn-block btn-primary" : "btn btn-block btn-outline-secondary")">Home</a>

    @foreach (string c in Model)
    {
    <a asp-controller="Home" asp-action="Index" asp-route-bookCategory="@c" asp-route-pageNum="1" 
       class="@(ViewBag.SelectedType == c ? "btn btn-block btn-primary" : "btn btn-block btn-outline-secondary")">@c</a>
    }
</div>